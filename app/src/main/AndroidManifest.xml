<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itemsplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activities.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity"
                 >
        </activity>
        <activity android:name=".activities.LoginActivity">
        </activity>
        <activity android:name=".activities.RegisterActivity">
        </activity>
        <activity android:name=".activities.ForgotPasswordActivity">
        </activity>
        <activity android:name=".activities.ItemsActivity">
        </activity>
        <activity android:name=".activities.ItemActivity">
        </activity>
        <activity android:name=".activities.MyItemsReservations">
        </activity>

    </application>

</manifest>